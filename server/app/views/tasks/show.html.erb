<div class='container'>
  <p id="notice"><%= notice %></p>
  
  <p>
  <strong>Name:</strong>
  <%= @task.name %>
  </p>

  <p>
  <strong>Path:</strong>
  <%= @task.path %>
  </p>

  <p>
  <strong>Start:</strong>
  <%= @task.start %>
  </p>

  <p>
  <strong>End:</strong>
  <%= @task.end %>
  </p>

  <p>
  <strong>Days:</strong>
  <%= @task.days %>
  </p>

  <p>
  <strong>Key:</strong>
  <%= @task.key %>
  </p>

  <%= link_to 'Edit', edit_task_path(@task), :class => 'btn btn-default' %> 
  <%= link_to 'Back', tasks_path, :class => 'btn btn-default'  %>
</div>
